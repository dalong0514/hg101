/* pages/type/type.wxss */
@import "/components/searchbar/searchbar.wxss";

.typedevice-box {
  width: 100%;
  height: 100%;
  /* background-color: black; */
  display: flex;
  flex-flow: wrap;
  align-content: flex-start;
  padding-top: 60px;
}

.typebox {
  display: flex;
  flex-flow: row;
  align-items: center;
  justify-content: center;
  height: 246rpx;
  width: 246rpx;
  background-color: white;
  /* border: 1px solid red; */
}
.typecontent {
  box-sizing: border-box;
  background-color: white;
  height: 200rpx;
  width: 200rpx;
  border-radius: 30rpx;
  border: 1px solid gray;
  font-size: 40rpx;
  text-align: center;
  line-height: 5em;
}